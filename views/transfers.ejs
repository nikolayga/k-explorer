<% include partials/header %>

<% include partials/navbar %>

<main role="main" class="col-md-9 ml-sm-auto col-lg-10 px-4 mt-3">
	<% include partials/navsmall %>
	<div style="background:#fff;">
		<table id="deposites" class="table table-striped table-bordered" style="width:100%">
		   <thead>
				<tr>
					<th>Date/Txhash</th>
					<th class="no-sort">Info</th>
					<th style="width:100px;">Ammount</th>
				</tr>
		   </thead>
		   <tbody>
				<% for(var i = 0; i < data.length; i++){ %>
				<tr>
					<td data-order="<%=moment(data[i]['date']).format('X') %>">
						<div class="max-90p"><a href="<%=tracker%>/tx/<%= data[i]['txhash'] %>" target="_blank"><%= data[i]['txhash'] %></a></div>
						<%= moment(data[i]['date']).fromNow()%> <small>( <%=moment(data[i]['date']).format('lll') %> )</small>
					</td>
					<td>
						<div class="clearfix">
							<div class="float-left">From &nbsp;</div>
							<div class="max-70p float-left">
								<a href="<%=tracker%>/address/<%= data[i]['from'] %>" target="_blank"><%= data[i]['from'] %></a>
							</div>
						</div>
						<div class="clearfix">
							<div class="float-left">To &nbsp;</div>
							<div class="max-70p float-left">
								<a href="<%=tracker%>/address/<%= data[i]['to'] %>" target="_blank"><%= data[i]['to'] %></a>
							</div>
						</div>
					</td>
					<td><div class="max-100"><img src="/images/tbtc_32.png">&nbsp; <%= Number((data[i]['value']).toFixed(6))  %></div></td>
				</tr>
				<%}%>
		   </tbody>
		</table>
    </div>
</main>


<% include partials/footer %>
